{
  "framework": "vite",
  "buildCommand": "cd apps/standalone-dashboard && npm run build",
  "outputDirectory": "apps/standalone-dashboard/dist",
  "installCommand": "cd apps/standalone-dashboard && npm install",
  "rootDirectory": "apps/standalone-dashboard",
  "env": {
    "VITE_STRICT_DATASOURCE": "true",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_STRICT_DATASOURCE": "true",
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Strict-Datasource",
          "value": "enabled"
        }
      ]
    }
  ]
}